{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "esModuleInterop": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "assets/*": [
        "./assets/*"
      ],
      "blueprints/*": [
        "./blueprints/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "nativewind"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}